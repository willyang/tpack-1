{
	"compilerOptions": {
		"outDir": "dist",
		"allowJs": true,
		"target": "esnext",
		"module": "commonjs",
		"moduleResolution": "node",
		"sourceMap": true,
		"newLine": "LF",
		"stripInternal": true,
		"preserveConstEnums": true,
		"allowSyntheticDefaultImports": true,
		"experimentalDecorators": true,
		"jsx": "react"
	}
}